{
  "name": "konnect-team-interview-frontend-exercise",
  "version": "0.0.0-development",
  "description": "Konnect Team Interview Frontend Exercise",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/konnect-team-interview-frontend-exercise.git"
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "server": "ts-node-dev ./server/index.ts",
    "build": "yarn typecheck && vite build",
    "commit": "cz",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:component": "cypress run --component -b chrome",
    "test:component:open": "cypress open --component -b chrome",
    "typecheck": "vue-tsc -p './tsconfig.build.json' --noEmit",
    "lint": "eslint '**/*.{js,jsx,ts,tsx,vue}'",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx,vue}' --fix"
  },
  "dependencies": {
    "@vueuse/components": "^9.10.0",
    "@vueuse/core": "^9.10.0",
    "@vueuse/head": "^1.0.22",
    "axios": "^0.27.2",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "io-ts": "^2.2.20",
    "lodash": "4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "unocss": "^0.48.0",
    "vue-demi": "^0.13.11", 
    "vue-router": "^4.1.6",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@cypress/vite-dev-server": "^3.4.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.5.1",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.15",
    "@types/flat": "^5.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "@vitest/ui": "^0.24.5",
    "@vue-macros/volar": "^0.5.7",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "commitizen": "^4.2.6",
    "cypress": "^10.11.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "eslint": "^8.31.0",
    "jsdom": "^20.0.3",
    "postcss-cli": "^10.1.0",
    "postcss-custom-properties": "^12.1.11",
    "sass": "^1.57.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-macros": "^1.2.2",
    "vite-plugin-pages": "^0.28.0",
    "vite-ssg": "^0.22.1",
    "vite": "^3.2.5",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.22"
  },
  "author": {
    "name": "Kong Inc.",
    "url": "https://konghq.com/"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  }
}
